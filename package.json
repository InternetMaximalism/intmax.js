{
  "name": "intmax",
  "version": "0.1.1",
  "description": "Intmax js client library",
  "main": "dist/index.js",
  "repository": "git@github.com:InternetMaximalism/intmax.js.git",
  "author": "Kazuki Kasahara <nuderebatsin.roal@gmail.com>",
  "license": "GPL-3.0",
  "keywords": [
    "intmax",
    "js",
    "blockchain",
    "crypto",
    "zk",
    "layer2"
  ],
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/InternetMaximalism/intmax.js/issues"
  },
  "homepage": "https://github.com/InternetMaximalism/intmax.js#readme",
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/node": "^14.18.25",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "circomlibjs": "git+https://github.com/InternetMaximalism/circomlibjs#fc534a12d9b34f2694ed0f75a686d080fc7a92d6",
    "ethers": "^5.7.0",
    "fake-indexeddb": "^4.0.0",
    "idb": "^7.0.2",
    "node-fetch": "^2.6.7"
  },
  "scripts": {
    "compile": "tsc -p ./tsconfig.json",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "dev": "npm run compile",
    "lint": "eslint --ext .js,.ts .",
    "fix": "npm run lint --fix",
    "test": "jest"
  },
  "engines": {
    "node": ">=14"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testTimeout": 60000,
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
